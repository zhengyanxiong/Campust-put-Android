            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.TagFlowActivity"/>
        <activity android:name=".activity.AlertViewDemoActivity"/>
        <activity android:name=".activity.HoverItemActivity"/>
        <activity android:name=".activity.DragBallActivity"/>
