        <activity android:name=".activity.PopupWindowActivity"/>
        <activity android:name=".activity.MyAccountActivity"/>
        <activity android:name=".activity.VideoCompressActivity"/>
        <activity android:name=".activity.PickerViewActivity"/>
        <activity android:name=".activity.JsonDataActivity"/>
        <activity android:name=".activity.PictureSelectorActivity"/>
        <activity android:name=".activity.KProgressHudActivity">
        </activity>
        <activity android:name=".activity.TipDialogActivity">
        </activity>
        <activity android:name=".activity.EmptyLayoutActivity">
        </activity>
        <activity android:name=".activity.ZxingActivity">
        </activity>
        <activity android:name=".activity.zxing.SecondActivity"/>
        <activity android:name=".activity.zxing.ThreeActivity"/>
        <activity android:name=".activity.HeaderListActivity">
        </activity>

    </application>

</manifest>